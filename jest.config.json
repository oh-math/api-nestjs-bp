{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
        "**/*.(t|j)s"
    ],
    "roots": [
        "<rootDir>"
    ],
    "modulePaths": [
        "<rootDir>"
    ],
    "moduleDirectories": [
        "node_modules"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
}